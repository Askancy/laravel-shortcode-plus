{
  "name": "askancy/laravel-shortcode-plus",
  "description": "A package that allows you to use custom shortcodes",
  "keywords": [
    "askancy",
    "laravel",
    "laravel-shortcode-plus",
    "shortcode",
    "shortcodes",
    "embed"
  ],
  "homepage": "https://github.com/askancy/laravel-shortcode-plus",
  "license": "MIT",
  "require": {
    "php": "^8.1|^8.2",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "^7.7",
    "illuminate/contracts": "^9.0|^10.0",
    "spatie/laravel-package-tools": "^1.13.0",
    "webwizo/laravel-shortcodes": "1.0.*"
  },
  "require-dev": {
    "laravel/pint": "^1.0",
    "nunomaduro/collision": "^6.0",
    "nunomaduro/larastan": "^2.0.1",
    "orchestra/testbench": "^7.0",
    "pestphp/pest": "^1.21",
    "pestphp/pest-plugin-laravel": "^1.1",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-phpunit": "^1.0",
    "phpunit/phpunit": "^9.5",
    "spatie/laravel-ray": "^1.31"
  },
  "autoload": {
    "psr-4": {
      "askancy\\LaravelShortcodePlus\\": "src",
      "askancy\\LaravelShortcodePlus\\Database\\Factories\\": "database/factories"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "askancy\\LaravelShortcodePlus\\Tests\\": "tests"
    }
  },
  "scripts": {
    "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
    "analyse": "vendor/bin/phpstan analyse",
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage",
    "format": "vendor/bin/pint"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "askancy\\LaravelShortcodePlus\\LaravelShortcodePlusServiceProvider"
      ],
      "aliases": {
        "LaravelShortcodePlus": "askancy\\LaravelShortcodePlus\\Facades\\LaravelShortcodePlus"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
